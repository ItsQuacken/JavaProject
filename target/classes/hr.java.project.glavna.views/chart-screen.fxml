<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.java.projekt.glavna.controllers.ChartDisplayController">
    <center>
        <WebView fx:id="webView" />
    </center>
    <bottom>
        <HBox spacing="10" padding="10">
            <Button text="Logout" onAction="#logoutButtonClicked" />
            <Button text="Switch to Prices" onAction="#switchToPricesButtonClicked" />
        </HBox>
    </bottom>
</BorderPane>
